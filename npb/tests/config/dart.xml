<?xml version='1.0' encoding='UTF-8'?>
<naif-pds4-bundle_configuration>

    <!-- =========================== -->
    <!-- PDS parameters              -->
    <!-- =========================== -->
    <pds_parameters>
        <pds_version>4</pds_version>
        <information_model>1.5.0.0</information_model>
        <xml_model>http://pds.nasa.gov/pds4/pds/v1/PDS4_PDS_1500.sch</xml_model>
        <schema_location>http://pds.nasa.gov/pds4/pds/v1 http://pds.nasa.gov/pds4/pds/v1/PDS4_PDS_1500.xsd
        </schema_location>
        <logical_identifier>urn:nasa:pds:dart.spice</logical_identifier>
        <context_products>
            <product name="DART">
                <type>Spacecraft</type>
                <lidvid>urn:nasa:pds:context:instrument_host:spacecraft.dart::1.0</lidvid>
            </product>
            <product name="LICIA">
                <type>Spacecraft</type>
                <lidvid>urn:nasa:pds:context:instrument_host:spacecraft.licia::1.0</lidvid>
            </product>            
            <product name="Earth">
                <type>Planet</type>
                <lidvid>urn:nasa:pds:context:target:planet.earth::1.0</lidvid>
            </product>   
            <product name="Didymos">
                <type>Asteroid</type>
                <lidvid>urn:nasa:pds:context:target:asteroid.didymos::1.0</lidvid>
            </product>
            <product name="Dimorphos">
                <type>Satellite</type>
                <lidvid>urn:nasa:pds:context:target:satellite.didymos.dimorphos::1.0</lidvid>
            </product>            
        </context_products>
    </pds_parameters>

    <!-- =========================== -->
    <!-- Bundle parameters           -->
    <!-- =========================== -->
    <bundle_parameters>
        <producer_name>Marc Costa Sitja</producer_name>
        <author_list>Costa Sitja M.; Semenov B. V.</author_list>
        <institution>NAIF/JPL</institution>
        <doi>10.17189/1522402</doi>
        <spiceds>../data/spiceds_dart.html</spiceds>
        <spice_name>DART</spice_name>
        <readme>
            <overview>
                The DART SPICE archive bundle contains observation geometry and
                other ancillary data in the form of SPICE System kernel files for
                the DART and LICIA spacecrafts, their instruments, and targets.
            </overview>
            <cognisant_persons>
                This archive bundle was produced by the Planetary Data System (PDS)
                Navigation and Ancillary Information Facility (NAIF) Node, Jet
                Propulsion Laboratory, Pasadena, California.
            </cognisant_persons>
        </readme>
        <!-- Optional parameters -->
        <!-- Increment start and stop times provided as a UTC calendar string.
             They require to be present. Use the following format:
             YYYY-MM-DDThh:mm:ssZ e.g., 2021-04-09T15:11:12Z. -->
        <increment_start>2021-11-09T00:00:00Z</increment_start>
        <increment_finish>2050-01-01T00:00:00Z</increment_finish>
        <kernels_to_load>
            <lsk>naif[0-9][0-9][0-9][0-9].tls</lsk>
            <sclk>dart_sclk_0000.tsc</sclk>
            <fk>dart_004.tf</fk>
        </kernels_to_load>
        <date_format>maklabel</date_format>
        <end_of_line>CRLF</end_of_line>
    </bundle_parameters>

    <!-- =========================== -->
    <!-- Mission Parameters          -->
    <!-- =========================== -->
    <mission_parameters>
        <mission_acronym>dart</mission_acronym>
        <mission_name>DART</mission_name>
        <mission_start>2021-11-09T00:00:00Z</mission_start>
        <mission_finish>2050-01-01T00:00:00Z</mission_finish>
        <observer>DART</observer>
        <target>Didymos</target>
        <secondary_observers>
            <observer>LICIA</observer>
        </secondary_observers>
        <secondary_targets>
            <target>Dimorphos</target>
            <target>Earth</target>
        </secondary_targets>
    </mission_parameters>

    <!-- =========================== -->
    <!-- Directories                 -->
    <!-- =========================== -->
    <directories>
        <working_directory>working</working_directory>
        <kernels_directory>kernels</kernels_directory>
        <staging_directory>staging</staging_directory>
        <final_directory>dart</final_directory>
        <reports_directory>working</reports_directory>
    </directories>

    <!-- =========================== -->
    <!-- Kernel List                 -->
    <!-- =========================== -->
    <kernel_list>
        <!-- The kernel list provides a number of patterns that are used -->
        <kernel pattern="naif[0-9][0-9][0-9][0-9].tls">
            <mklabel_options>DEF_TIMES</mklabel_options>
            <description>SPICE LSK file incorporating leapseconds up to $DATE, created by NAIF, JPL.</description>
            <patterns>
                <DATE value="naif0012.tls">2017-JAN-01</DATE>
            </patterns>
        </kernel>
        <kernel pattern="pck[0-9][0-9][0-9][0-9][0-9].tpc">
            <mklabel_options>DEF_TIMES</mklabel_options>
            <description>SPICE text PCK file containing constants from the $REPORT report, created by NAIF, JPL.
            </description>
            <patterns>
                <REPORT value="pck00010.tpc">IAU 2009</REPORT>
            </patterns>
        </kernel>
        <kernel pattern="didymosSystem_d300_s101_01.tpc">
            <mklabel_options>DEF_TIMES</mklabel_options>
            <description>SPICE text PCK file containing constants for the Didymos system, created by the DART
                science team at the Johns Hopkins University Applied Physics Laboratory.
            </description>
            <observers>
                <observer>DART</observer>
                <observer>LICIA</observer>
            </observers>
            <targets>
                <target>Didymos</target>
                <target>Dimorphos</target>
            </targets>
        </kernel>
        <kernel pattern="dart_[0-9][0-9][0-9].tf">
            <mklabel_options>DEF_TIMES</mklabel_options>
            <description>SPICE FK file defining reference frames for the DART spacecraft, its structures, and science
                instruments, created by NAIF, JPL.
            </description>
        </kernel>
        <kernel pattern="licia_[0-9][0-9][0-9].tf">
            <mklabel_options>DEF_TIMES</mklabel_options>
            <description>SPICE FK file defining reference frames for the LICIA spacecraft, its structures, and science
                instruments, created by NAIF, JPL.
            </description>
            <observers>
                <observer>LICIA</observer>
            </observers>
            <targets>
                <target>Didymos</target>
                <target>Dimorphos</target>
            </targets>  
        </kernel>        
        <kernel pattern="dart_draco_[0-9][0-9][0-9].ti">
            <mklabel_options>DEF_TIMES</mklabel_options>
            <description>SPICE IK file providing FOV definitions for the DART Didymos
                Reconnaissance and Asteroid Camera for OpNav (DRACO) instrument, created by NAIF, JPL.
            </description>
            <targets>
                <target>Didymos</target>
                <target>Dimorphos</target>
            </targets>  
        </kernel>
        <kernel pattern="licia_pl_[0-9][0-9][0-9].ti">
            <mklabel_options>DEF_TIMES</mklabel_options>
            <description>SPICE IK file providing FOV definitions for the LICIA Luke and Leia instruments, 
                created by the DART science team at the Johns Hopkins University Applied Physics Laboratory.
            </description>
            <observers>
                <observer>LICIA</observer>
            </observers>
            <targets>
                <target>Didymos</target>
                <target>Dimorphos</target>
            </targets>              
        </kernel>
        <kernel pattern="dart_sclk_[0-9][0-9][0-9][0-9].tsc">
            <mklabel_options>DEF_TIMES</mklabel_options>
            <description>SPICE SCLK file containing actual time correlation data for the DART on-board clock, created
                by the DART science team at the Johns Hopkins University Applied Physics Laboratory.
            </description>
        </kernel>
        <kernel pattern="licia_sclk_[0-9][0-9][0-9][0-9].tsc">
            <mklabel_options>DEF_TIMES</mklabel_options>
            <description>SPICE SCLK file containing actual time correlation data for the LICIA on-board clock, created
                by the DART science team at the Johns Hopkins University Applied Physics Laboratory.
            </description>
            <observers>
                <observer>LICIA</observer>
            </observers>
            <targets>
                <target>Didymos</target>
                <target>Dimorphos</target>
            </targets>              
        </kernel>
        <kernel pattern="de430.bsp">
            <mklabel_options> </mklabel_options>
            <description>SPICE SPK file containing JPL planetary ephemerides version DE430, created by the Solar
                System Dynamics group (SSD), JPL.
            </description>
        </kernel>
        <kernel pattern="didymos_barycenter_d300_01.bsp">
            <mklabel_options> </mklabel_options>
            <description>SPICE SPK file containing reconstructed LADEE trajectory, covering the whole phasing loop phase
                plus the latter part of the launch phase, created by NAIF, JPL.
            </description>
            <observers>
                <observer>DART</observer>
                <observer>LICIA</observer>
            </observers>
            <targets>
                <target>Didymos</target>
                <target>Dimorphos</target>
            </targets>            
        </kernel>
        <kernel pattern="didymos_d300_s101_01.bsp">
            <mklabel_options> </mklabel_options>
            <description>SPICE SPK file containing reconstructed LADEE trajectory, covering the lunar orbit
                acquisition and instrument commissioning phases, created by NAIF, JPL.
            </description>
            <observers>
                <observer>DART</observer>
                <observer>LICIA</observer>
            </observers>
            <targets>
                <target>Didymos</target>
                <target>Dimorphos</target>
            </targets>            
        </kernel>
        <kernel pattern="dimorphos_d300_s101_01.bsp">
            <mklabel_options> </mklabel_options>
            <description>SPICE SPK file containing reconstructed LADEE trajectory, covering the whole primary and extended
                 science phases up until approximate lunar impact, created by NAIF, JPL.
            </description>
            <observers>
                <observer>DART</observer>
                <observer>LICIA</observer>
            </observers>
            <targets>
                <target>Didymos</target>
                <target>Dimorphos</target>
            </targets>            
        </kernel>
        <kernel pattern="D210426-L211124-EP-RA26-V01_traj.bsp">
            <mklabel_options> </mklabel_options>
            <description>SPICE SPK file providing the location of LADEE at its end-of-mission impact site, covering from the approximate
                lunar impact time to 2099-01-01, created by NAIF, JPL.
            </description>
        </kernel>
        <kernel pattern="D210426-L211124-EP-RA26-V01_impact.bc">
            <mklabel_options> </mklabel_options>
            <description>SPICE SPK file providing the location of LADEE at its end-of-mission impact site, covering from the approximate
                lunar impact time to 2099-01-01, created by NAIF, JPL.
            </description>
             <targets>
                <target>Didymos</target>
                <target>Dimorphos</target>
            </targets>    
        </kernel>        
        <kernel pattern="D210426-L211124-EP-RA26-V01_impact.bc">
            <mklabel_options> </mklabel_options>
            <description>SPICE CK file containing reconstructed LADEE spacecraft orientation, created by NAIF,
                JPL.
            </description>
            <targets>
                <target>Didymos</target>
                <target>Dimorphos</target>
            </targets>               
        </kernel>
        <kernel pattern="dart_[0-9][0-9][0-9].tm">
            <mklabel_options> </mklabel_options>
            <description>SPICE MK file listing kernels for the whole mission, created by NAIF, JPL.
            </description>
            <observers>
                <observer>DART</observer>
                <observer>LICIA</observer>
            </observers>
            <targets>
                <target>Didymos</target>
                <target>Dimorphos</target>
            </targets>   
        </kernel>
    </kernel_list>

    <!-- =========================== -->
    <!-- Meta-kernel                 -->
    <!-- =========================== -->
    <meta-kernel>
        <!-- A resulting meta-kernel will look like -->
        <mk_inputs>
            <file>kernels/mk/dart_000.tm</file>
        </mk_inputs>
    </meta-kernel>
</naif-pds4-bundle_configuration>