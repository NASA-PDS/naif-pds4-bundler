PDS_VERSION_ID             = PDS3
VOLUME_ID                  = NSYTSP_1000
RECORD_TYPE                = FIXED_LENGTH
RECORD_BYTES               = 226
FILE_RECORDS               = 57
^INDEX_TABLE               = "DSINDEX.TAB"

OBJECT                     = INDEX_TABLE

  INTERCHANGE_FORMAT       = ASCII
  ROW_BYTES                = 226
  ROWS                     = 57
  COLUMNS                  = 10
  INDEX_TYPE               = SINGLE
  INDEXED_FILE_NAME        = { 
                               "*.tf",
                               "*.tls",
                               "*.ti",
                               "*.bc",
                               "*.bsp",
                               "*.tsc",
                               "*.tpc"
                             }

  OBJECT                   = COLUMN
    NAME                   = START_TIME
    DATA_TYPE              = "TIME"
    START_BYTE             = 1
    BYTES                  = 23
    FORMAT                 = "A23"
    DESCRIPTION            = "Start time of the product."
  END_OBJECT               = COLUMN

  OBJECT                   = COLUMN
    NAME                   = STOP_TIME
    DATA_TYPE              = "TIME"
    START_BYTE             = 25
    BYTES                  = 23
    FORMAT                 = "A23"
    DESCRIPTION            = "Stop time of the product."
  END_OBJECT               = COLUMN

  OBJECT                   = COLUMN
    NAME                   = FILE_SPECIFICATION_NAME
    DATA_TYPE              = "CHARACTER"
    START_BYTE             = 50
    BYTES                  = 54
    FORMAT                 = "A54"
    DESCRIPTION            = "Unix style path and label file name."
  END_OBJECT               = COLUMN

  OBJECT                   = COLUMN
    NAME                   = DATA_SET_ID
    DATA_TYPE              = "CHARACTER"
    START_BYTE             = 107
    BYTES                  = 19
    FORMAT                 = "A19"
    DESCRIPTION            = "Data set ID."
  END_OBJECT               = COLUMN

  OBJECT                   = COLUMN
    NAME                   = PRODUCT_CREATION_TIME
    DATA_TYPE              = "TIME"
    START_BYTE             = 128
    BYTES                  = 19
    FORMAT                 = "A19"
    DESCRIPTION            = "Product creation time."
  END_OBJECT               = COLUMN

  OBJECT                   = COLUMN
    NAME                   = RELEASE_ID
    DATA_TYPE              = "CHARACTER"
    START_BYTE             = 149
    BYTES                  = 4
    FORMAT                 = "A4"
    DESCRIPTION            = "Identifier for product release."
  END_OBJECT               = COLUMN

  OBJECT                   = COLUMN
    NAME                   = RELEASE_DATE
    DATA_TYPE              = "DATE"
    START_BYTE             = 155
    BYTES                  = 10
    FORMAT                 = "A10"
    DESCRIPTION            = "Date on which the product was released."
  END_OBJECT               = COLUMN

  OBJECT                   = COLUMN
    NAME                   = KERNEL_TYPE_ID
    DATA_TYPE              = "CHARACTER"
    START_BYTE             = 167
    BYTES                  = 4
    FORMAT                 = "A4"
    DESCRIPTION            = "Kernel type."
  END_OBJECT               = COLUMN

  OBJECT                   = COLUMN
    NAME                   = PRODUCT_ID
    DATA_TYPE              = "CHARACTER"
    START_BYTE             = 174
    BYTES                  = 36
    FORMAT                 = "A36"
    DESCRIPTION            = "Kernel file name."
  END_OBJECT               = COLUMN

  OBJECT                   = COLUMN
    NAME                   = VOLUME_ID
    DATA_TYPE              = "CHARACTER"
    START_BYTE             = 213
    BYTES                  = 11
    FORMAT                 = "A11"
    DESCRIPTION            = "The volume containing this data file."
  END_OBJECT               = COLUMN

END_OBJECT                 = INDEX_TABLE
END
