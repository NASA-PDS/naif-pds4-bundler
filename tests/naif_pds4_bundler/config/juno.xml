<?xml version='1.0' encoding='UTF-8'?>
<naif-pds4-bundler_configuration>

    <!-- =========================== -->
    <!-- PDS parameters              -->
    <!-- =========================== -->
    <pds_parameters>
        <pds_version>3</pds_version>
    </pds_parameters>

    <!-- =========================== -->
    <!-- Bundle parameters           -->
    <!-- =========================== -->
    <bundle_parameters>
        <producer_name>Marc Costa Sitja</producer_name>
        <institution>NAIF/JPL</institution>
        <volume_id>jnosp_1000</volume_id>
        <spice_name>JUNO</spice_name>
    </bundle_parameters>

    <!-- =========================== -->
    <!-- Mission Parameters          -->
    <!-- =========================== -->
    <mission_parameters>
        <mission_acronym>juno</mission_acronym>
        <mission_name>JUNO</mission_name>
        <observer>JUNO</observer>
        <target>Jupiter</target>
        <pds3_mission_template>
            <!-- Extracted from juno_mission_template_pds.v1.1               -->
            <!--                                                             -->
            <!-- MAKLABEL (ver. 5.1.0+) Mission Template File for JUNO       -->
            <!-- Version 1.0 ~~ by Boris Semenov, NAIF/JPL ~~ May 24, 2017   -->
            <!--                                                             -->
            <!--    Initial version                                          -->
            <!--                                                             -->
            <!-- Version 1.1 ~~ by Boris Semenov, NAIF/JPL ~~ Dec.11, 2017   -->
            <!--                                                             -->
            <!--    Added TEAM_FGM.                                          -->
            <!--                                                             -->
            <!-- Version 1.2 ~~ by Marc Costa Sitja NAIF/JPL ~~ Mar. 9, 2022 -->
            <!--                                                             -->
            <!--    Updated Mission Phases.                                  -->
            <!--                                                             -->
            <MISSION_NAME>"JUNO"</MISSION_NAME>
            <SPACECRAFT_NAME>"JUNO"</SPACECRAFT_NAME>
            <TARGET_NAME>"JUPITER"</TARGET_NAME>
            <DATA_SET_ID>"JNO-J/E/SS-SPICE-6-V1.0"</DATA_SET_ID>
            <PRODUCT_VERSION_TYPE>"ACTUAL"</PRODUCT_VERSION_TYPE>
            <MISSION_PHASE_NAME>"N/A"</MISSION_PHASE_NAME>
            <SOURCE_PRODUCT_ID>"N/A"</SOURCE_PRODUCT_ID>
            <INSTRUMENT_NAME>"N/A"</INSTRUMENT_NAME>
            <PLATFORM_OR_MOUNTING_NAME>"N/A"</PLATFORM_OR_MOUNTING_NAME>
            <NOTE>"See comments in the file for details"</NOTE>
            <maklabel_options>
                    <!--                                                     -->
                    <!-- Label keys for mission phases.                      -->
                    <!--                                                     -->
                    <!-- From JUNO MISSION.CAT ver. 2022-01-21               -->
                    <!-- JUNO: Kurth, Bolton Revision - XM"                  -->
                    <!--                                                     -->
                    <!--  Mission Phases                                     -->
                    <!--  ==============                                     -->
                    <!--                                                     -->
                    <!--  LAUNCH                                             -->
                    <!--  ~~~~~~                                             -->
                    <!--    Mission Phase Start Time :  2011-08-05 (2011-217)-->
                    <!--    Mission Phase Stop Time  :  2011-08-08 (2011-220)-->
                    <!--                                                     -->
                    <!--  INNER CRUISE 1                                     -->
                    <!--  ~~~~~~~~~~~~~~                                     -->
                    <!--    Mission Phase Start Time :  2011-08-08 (2011-220)-->
                    <!--    Mission Phase Stop Time  :  2011-10-10 (2011-283)-->
                    <!--                                                     -->
                    <!--  INNER CRUISE 2                                     -->
                    <!--  ~~~~~~~~~~~~~~                                     -->
                    <!--    Mission Phase Start Time :  2011-10-10 (2011-283)-->
                    <!--    Mission Phase Stop Time  :  2013-05-29 (2013-149)-->
                    <!--                                                     -->
                    <!--  INNER CRUISE 3                                     -->
                    <!--  ~~~~~~~~~~~~~~                                     -->
                    <!--    Mission Phase Start Time :  2013-05-29 (2013-149)-->
                    <!--    Mission Phase Stop Time  :  2013-11-05 (2013-309)-->
                    <!--                                                     -->
                    <!--  OUTER CRUISE                                       -->
                    <!--  ~~~~~~~~~~~~~~                                     -->
                    <!--    Mission Phase Start Time :  2013-11-05 (2013-309)-->
                    <!--    Mission Phase Stop Time  :  2016-01-05 (2016-005)-->
                    <!--                                                     -->
                    <!--  JUPITER APPROACH                                   -->
                    <!--  ~~~~~~~~~~~~~~                                     -->
                    <!--    Mission Phase Start Time :  2016-01-05 (2016-005)-->
                    <!--    Mission Phase Stop Time  :  2016-07-01 (2016-183)-->
                    <!--                                                     -->
                    <!--  JUPITER ORBIT INSERTION                            -->
                    <!--  ~~~~~~~~~~~~~~~~~~~~~~~~~~~~                       -->
                    <!--    Mission Phase Start Time :  2016-07-01 (2016-183)-->
                    <!--    Mission Phase Stop Time  :  2016-07-05 (2016-187)-->
                    <!--                                                     -->
                    <!--  SCIENCE ORBITS                                     -->
                    <!--  ~~~~~~~~~~~~~~                                     -->
                    <!--    Mission Phase Start Time :  2016-07-05 (2016-187)-->
                    <!--    Mission Phase Stop Time  :  2021-07-03 (2021-184)-->
                    <!--                                                     -->
                    <!--  EXTENDED MISSION                                   -->
                    <!--  ~~~~~~~~~~~~~~~~                                   -->
                    <!--    Mission Phase Start Time :  2021-07-03 (2021-184)-->
                    <!--    Mission Phase Stop Time  :  2025-09-17 (2021-260)-->
                    <!--                                                     -->
                    <!--  DEORBIT                                            -->
                    <!--  ~~~~~~~                                            -->
                    <!--    Mission Phase Start Time :  2025-09-17 (2021-260)-->
                    <!--    Mission Phase Stop Time  :  ????-??-?? (????-???)-->
                    <!--                                                     -->
                    <PHASE_ORB>
                        <MISSION_PHASE_NAME>"SCIENCE ORBITS"</MISSION_PHASE_NAME>
                    </PHASE_ORB>
                    <PHASE_ORB_EXT>
                        <MISSION_PHASE_NAME>"SCIENCE ORBITS",
                                            "EXTENDED MISSION"</MISSION_PHASE_NAME>
                    </PHASE_ORB_EXT>
                    <PHASE_EXT>
                        <MISSION_PHASE_NAME>"EXTENDED MISSION"</MISSION_PHASE_NAME>
                    </PHASE_EXT>
                    <PHASE_ALL>
                        <MISSION_PHASE_NAME>"LAUNCH",
                                            "INNER CRUISE 1",
                                            "INNER CRUISE 2",
                                            "INNER CRUISE 3",
                                            "OUTER CRUISE",
                                            "JUPITER APPROACH",
                                            "JUPITER ORBIT INSERTION",
                                            "SCIENCE ORBITS",
                                            "EXTENDED MISSION",
                                            "DEORBIT"
                        </MISSION_PHASE_NAME>
                    </PHASE_ALL>
                <!--                                                         -->
                <!-- Label keys for producer ids                             -->
                <!--                                                         -->
                <TEAM_NAIF>
                    <PRODUCER_ID>"NAIF/JPL"</PRODUCER_ID>
                </TEAM_NAIF>
                <TEAM_NAV>
                    <PRODUCER_ID>"JUNO NAV/JPL"</PRODUCER_ID>
                </TEAM_NAV>
                <!--                                                         -->
                <!-- Label keys for product version types                    -->
                <!--                                                         -->
                <ACTUAL>
                    <PRODUCT_VERSION_TYPE>"ACTUAL"</PRODUCT_VERSION_TYPE>
                </ACTUAL>
                <PREDICT>
                    <PRODUCT_VERSION_TYPE>"PREDICT"</PRODUCT_VERSION_TYPE>
                </PREDICT>
                <!--                                                         -->
                <!-- Label keys for instrument/mounting platform names.      -->
                <!--                                                         -->
                <INST_SC>
                    <INSTRUMENT_NAME>"JUNO SPACECRAFT"</INSTRUMENT_NAME>
                    <PLATFORM_OR_MOUNTING_NAME>"N/A"</PLATFORM_OR_MOUNTING_NAME>
                </INST_SC>
                <INST_MAG>
                    <INSTRUMENT_NAME>"MAGNETOMETER"</INSTRUMENT_NAME>
                    <PLATFORM_OR_MOUNTING_NAME>"JUNO SPACECRAFT"</PLATFORM_OR_MOUNTING_NAME>
                </INST_MAG>
                <INST_UVS>
                    <INSTRUMENT_NAME>"ULTRAVIOLET SPECTROGRAPH"</INSTRUMENT_NAME>
                    <PLATFORM_OR_MOUNTING_NAME>"JUNO SPACECRAFT"</PLATFORM_OR_MOUNTING_NAME>
                </INST_UVS>
            </maklabel_options>
        </pds3_mission_template>
        <kernels_to_load>
            <lsk>naif[0-9][0-9][0-9][0-9].tls</lsk>
            <sclk>JNO_SCLKSCET.[0-9][0-9][0-9][0-9][0-9].tsc</sclk>
            <fk>juno_v[0-9][0-9].tf</fk>
        </kernels_to_load>
    </mission_parameters>

    <!-- =========================== -->
    <!-- Directories                 -->
    <!-- =========================== -->
    <directories>
        <working_directory>working</working_directory>
        <kernels_directory>kernels</kernels_directory>
        <staging_directory>staging</staging_directory>
        <bundle_directory>bundle</bundle_directory>
        <orbnum_directory>misc/orbnum</orbnum_directory>
    </directories>


    <!-- =========================== -->
    <!-- Kernel List                 -->
    <!-- =========================== -->
    <kernel_list>
        <kernel pattern="jno_sclkscet_[0-9][0-9][0-9][0-9][0-9].tsc">
            <mapping>JNO_SCLKSCET.$VERSION.tsc</mapping>
            <mklabel_options>TEAM_NAIF</mklabel_options>
            <description>SPICE SCLK file containing time correlation data for the main JUNO on-board clock, created by NAIF, JPL. The original name of this file was JNO_SCLKSCET.$VERSION.tsc.
            </description>
            <patterns>
                <VERSION pattern="KERNEL">JNO_SCLKSCET.$VERSION.tsc</VERSION>
            </patterns>
        </kernel>
        <kernel pattern="jup[0-9][0-9][0-9]s.bsp">
            <mklabel_options>PHASE_ALL TEAM_NAIF</mklabel_options>
            <description>SPICE SPK file containing JPL Jovian satellite ephemerides version JUP$VERSION, created by NAIF, JPL.
            </description>
            <patterns>
                <VERSION pattern="KERNEL">jup$VERSIONs.bsp</VERSION>
            </patterns>
        </kernel>

        <!-- Special case: Two SPKs of the same nature need different        -->
        <!-- Phases and therefore we need an entry for each file.            -->
        <kernel pattern="juno_rec_2106[0-9][0-9]_[0-9][0-9][0-9][0-9][0-9][0-9]_[0-9][0-9][0-9][0-9][0-9][0-9].bsp">
            <mklabel_options>TEAM_NAV PHASE_ORB_EXT</mklabel_options>
            <description>SPICE SPK file containing reconstructed trajectory of the JUNO spacecraft, created by JUNO NAV Team, JPL. The original name of this file was spk_rec_2106$START_$STOP_$CREATION.bsp.
            </description>
            <patterns>
                <START pattern="KERNEL">juno_rec_2106$START_[0-9][0-9][0-9][0-9][0-9][0-9]_[0-9][0-9][0-9][0-9][0-9][0-9].bsp</START>
                <STOP pattern="KERNEL">juno_rec_2106[0-9][0-9]_$STOP_[0-9][0-9][0-9][0-9][0-9][0-9].bsp</STOP>
                <CREATION pattern="KERNEL">juno_rec_2106[0-9][0-9]_[0-9][0-9][0-9][0-9][0-9][0-9]_$CREATION.bsp</CREATION>
            </patterns>
        </kernel>
        <kernel pattern="juno_rec_2108[0-9][0-9]_[0-9][0-9][0-9][0-9][0-9][0-9]_[0-9][0-9][0-9][0-9][0-9][0-9].bsp">
            <mklabel_options>TEAM_NAV PHASE_EXT</mklabel_options>
            <description>SPICE SPK file containing reconstructed trajectory of the JUNO spacecraft, created by JUNO NAV Team, JPL. The original name of this file was spk_rec_2108$START_$STOP_$CREATION.bsp.
            </description>
            <patterns>
                <START pattern="KERNEL">juno_rec_2108$START_[0-9][0-9][0-9][0-9][0-9][0-9]_[0-9][0-9][0-9][0-9][0-9][0-9].bsp</START>
                <STOP pattern="KERNEL">juno_rec_2108[0-9][0-9]_$STOP_[0-9][0-9][0-9][0-9][0-9][0-9].bsp</STOP>
                <CREATION pattern="KERNEL">juno_rec_2108[0-9][0-9]_[0-9][0-9][0-9][0-9][0-9][0-9]_$CREATION.bsp</CREATION>
            </patterns>
        </kernel>


        <!-- Special case: a number of CKs of the same nature need different -->
        <!-- Phases and therefore we need an entry for each "month".         -->
        <kernel pattern="juno_sc_rec_2106[0-9]{2}_2106[0-9]{2}_v[0-9]{2}.bc">
            <mklabel_options>PHASE_ORB INST_SC TEAM_NAIF</mklabel_options>
            <description>SPICE CK file containing reconstructed orientation of the JUNO spacecraft, created by NAIF, JPL using data from an ATT_RECON file produced by SCT, LMCO.
            </description>
        </kernel>
        <kernel pattern="juno_sc_rec_2106[0-9]{2}_2107[0-9]{2}_v[0-9]{2}.bc">
            <mklabel_options>PHASE_ORB_EXT INST_SC TEAM_NAIF</mklabel_options>
            <description>SPICE CK file containing reconstructed orientation of the JUNO spacecraft, created by NAIF, JPL using data from an ATT_RECON file produced by SCT, LMCO.
            </description>
        </kernel>
        <kernel pattern="juno_sc_rec_2107[0-9]{2}_[0-9]{6}_v[0-9]{2}.bc">
            <mklabel_options>PHASE_EXT INST_SC TEAM_NAIF</mklabel_options>
            <description>SPICE CK file containing reconstructed orientation of the JUNO spacecraft, created by NAIF, JPL using data from an ATT_RECON file produced by SCT, LMCO.
            </description>
        </kernel>
        <kernel pattern="juno_sc_rec_2108[0-9]{2}_[0-9]{6}_v[0-9]{2}.bc">
            <mklabel_options>PHASE_EXT INST_SC TEAM_NAIF</mklabel_options>
            <description>SPICE CK file containing reconstructed orientation of the JUNO spacecraft, created by NAIF, JPL using data from an ATT_RECON file produced by SCT, LMCO.
            </description>
        </kernel>

        <kernel pattern="juno_chub_rec_2106[0-9]{2}_2106[0-9]{2}_v[0-9]{2}.bc">
            <mklabel_options>PHASE_ORB INST_SC TEAM_NAIF</mklabel_options>
            <description>SPICE CK file containing reconstructed orientation of the MAG ASC CHU-B, created by NAIF, JPL using CHU-B channelized telemetry quaternions.
            </description>
        </kernel>
        <kernel pattern="juno_chuc_rec_2106[0-9]{2}_2106[0-9]{2}_v[0-9]{2}.bc">
            <mklabel_options>PHASE_ORB INST_SC TEAM_NAIF</mklabel_options>
            <description>SPICE CK file containing reconstructed orientation of the MAG ASC CHU-C, created by NAIF, JPL using CHU-C channelized telemetry quaternions.
            </description>
        </kernel>
        <kernel pattern="juno_chud_rec_2106[0-9]{2}_2106[0-9]{2}_v[0-9]{2}.bc">
            <mklabel_options>PHASE_ORB INST_SC TEAM_NAIF</mklabel_options>
            <description>SPICE CK file containing reconstructed orientation of the MAG ASC CHU-D, created by NAIF, JPL using CHU-D channelized telemetry quaternions.
            </description>
        </kernel>
        <kernel pattern="juno_chub_rec_2106[0-9]{2}_2107[0-9]{2}_v[0-9]{2}.bc">
            <mklabel_options>PHASE_ORB_EXT INST_SC TEAM_NAIF</mklabel_options>
            <description>SPICE CK file containing reconstructed orientation of the MAG ASC CHU-B, created by NAIF, JPL using CHU-B channelized telemetry quaternions.
            </description>
        </kernel>
        <kernel pattern="juno_chuc_rec_2106[0-9]{2}_2107[0-9]{2}_v[0-9]{2}.bc">
            <mklabel_options>PHASE_ORB_EXT INST_SC TEAM_NAIF</mklabel_options>
            <description>SPICE CK file containing reconstructed orientation of the MAG ASC CHU-C, created by NAIF, JPL using CHU-C channelized telemetry quaternions.
            </description>
        </kernel>
        <kernel pattern="juno_chud_rec_2106[0-9]{2}_2107[0-9]{2}_v[0-9]{2}.bc">
            <mklabel_options>PHASE_ORB_EXT INST_SC TEAM_NAIF</mklabel_options>
            <description>SPICE CK file containing reconstructed orientation of the MAG ASC CHU-D, created by NAIF, JPL using CHU-D channelized telemetry quaternions.
            </description>
        </kernel>
        <kernel pattern="juno_chub_rec_2107[0-9]{2}_[0-9]{6}_v[0-9]{2}.bc">
            <mklabel_options>PHASE_EXT INST_SC TEAM_NAIF</mklabel_options>
            <description>SPICE CK file containing reconstructed orientation of the MAG ASC CHU-B, created by NAIF, JPL using CHU-B channelized telemetry quaternions.
            </description>
        </kernel>
        <kernel pattern="juno_chuc_rec_2107[0-9]{2}_[0-9]{6}_v[0-9]{2}.bc">
            <mklabel_options>PHASE_EXT INST_SC TEAM_NAIF</mklabel_options>
            <description>SPICE CK file containing reconstructed orientation of the MAG ASC CHU-C, created by NAIF, JPL using CHU-C channelized telemetry quaternions.
            </description>
        </kernel>
        <kernel pattern="juno_chud_rec_2107[0-9]{2}_[0-9]{6}_v[0-9]{2}.bc">
            <mklabel_options>PHASE_EXT INST_SC TEAM_NAIF</mklabel_options>
            <description>SPICE CK file containing reconstructed orientation of the MAG ASC CHU-D, created by NAIF, JPL using CHU-D channelized telemetry quaternions.
            </description>
        </kernel>
        <kernel pattern="juno_chub_rec_2108[0-9]{2}_[0-9]{6}_v[0-9]{2}.bc">
            <mklabel_options>PHASE_EXT INST_SC TEAM_NAIF</mklabel_options>
            <description>SPICE CK file containing reconstructed orientation of the MAG ASC CHU-B, created by NAIF, JPL using CHU-B channelized telemetry quaternions.
            </description>
        </kernel>
        <kernel pattern="juno_chuc_rec_2108[0-9]{2}_[0-9]{6}_v[0-9]{2}.bc">
            <mklabel_options>PHASE_EXT INST_SC TEAM_NAIF</mklabel_options>
            <description>SPICE CK file containing reconstructed orientation of the MAG ASC CHU-C, created by NAIF, JPL using CHU-C channelized telemetry quaternions.
            </description>
        </kernel>
        <kernel pattern="juno_chud_rec_2108[0-9]{2}_[0-9]{6}_v[0-9]{2}.bc">
            <mklabel_options>PHASE_EXT INST_SC TEAM_NAIF</mklabel_options>
            <description>SPICE CK file containing reconstructed orientation of the MAG ASC CHU-D, created by NAIF, JPL using CHU-D channelized telemetry quaternions.
            </description>
        </kernel>
        <kernel pattern="juno_mobib_rec_2106[0-9]{2}_2106[0-9]{2}_v[0-9]{2}.bc">
            <mklabel_options>PHASE_ORB INST_MAG TEAM_NAIF</mklabel_options>
            <description>SPICE CK file containing reconstructed orientation of the MAG IN-BOARD MOB, created by NAIF, JPL using channelized telemetry quaternions from two CHUs mounted on it.
            </description>
        </kernel>
        <kernel pattern="juno_mobib_rec_2106[0-9]{2}_2107[0-9]{2}_v[0-9]{2}.bc">
            <mklabel_options>PHASE_ORB_EXT INST_MAG TEAM_NAIF</mklabel_options>
            <description>SPICE CK file containing reconstructed orientation of the MAG IN-BOARD MOB, created by NAIF, JPL using channelized telemetry quaternions from two CHUs mounted on it.
            </description>
        </kernel>
        <kernel pattern="juno_mobib_rec_2107[0-9]{2}_[0-9]{6}_v[0-9]{2}.bc">
            <mklabel_options>PHASE_EXT INST_MAG TEAM_NAIF</mklabel_options>
            <description>SPICE CK file containing reconstructed orientation of the MAG IN-BOARD MOB, created by NAIF, JPL using channelized telemetry quaternions from two CHUs mounted on it.
            </description>
        </kernel>
        <kernel pattern="juno_mobib_rec_2108[0-9]{2}_[0-9]{6}_v[0-9]{2}.bc">
            <mklabel_options>PHASE_EXT INST_MAG TEAM_NAIF</mklabel_options>
            <description>SPICE CK file containing reconstructed orientation of the MAG IN-BOARD MOB, created by NAIF, JPL using channelized telemetry quaternions from two CHUs mounted on it.
            </description>
        </kernel>

        <kernel pattern="juno_uvs_rec_[0-9]{6}_[0-9]{6}_v[0-9]{2}.bc">
            <mklabel_options>PHASE_ORB_EXT INST_UVS TEAM_NAIF</mklabel_options>
            <description>SPICE CK file containing reconstructed orientation of the JUNO UVS instrument internal mirror, created by NAIF, JPL by merging data from individual UVS CKs produced by the UVS Team, SWRI.
            </description>
        </kernel>
        <kernel pattern="juno_mobib_ali_[0-9]{6}_[0-9]{6}_avg_v[0-9]{2}.bc">
            <mklabel_options>PHASE_ORB_EXT INST_MAG TEAM_NAIF</mklabel_options>
            <description>SPICE CK file containing averaged time-varying alignment of the MAG $MOB MOB with respect to the spacecraft frame, created by NAIF.
            </description>
        </kernel>

        <!-- Testing patterns with wildcards in values -->
        <kernel pattern="^juno_mob[a-z]b_ali_[0-9]{6}_[0-9]{6}_thm_v[0-9]{2}.bc">
            <mklabel_options>PHASE_ORB_EXT INST_SC TEAM_NAIF</mklabel_options>
            <description>SPICE CK file containing time-varying alignment of the MAG $MOB MOB with respect to the spacecraft frame, based on solar array and SRU bracket temperature-induced flexing model by DTU, created by FGM Team, GSFC.
            </description>
            <patterns>
                <MOB value="juno_mobib_ali">IN-BOARD</MOB>
                <MOB value="juno_mobob_ali">OUT-BOARD</MOB>
            </patterns>
        </kernel>
        <kernel pattern="juno_chu[a-z]{1}_ali_[0-9]{6}_[0-9]{6}_avg_v[0-9]{2}.bc">
            <mklabel_options>PHASE_ORB_EXT INST_MAG TEAM_NAIF</mklabel_options>
            <description> SPICE CK file containing averaged time-varying alignment of the MAG ASC CHU-$CHU with respect to the spacecraft frame, created by NAIF.
            </description>
            <patterns>
                <CHU value="juno_chub_ali">B</CHU>
                <CHU value="juno_chuc_ali">C</CHU>
                <CHU value="juno_chud_ali">D</CHU>
            </patterns>
        </kernel>
        <kernel pattern="juno_chu[a-z]{1}_ali_[0-9]{6}_[0-9]{6}_avg_v[0-9]{2}.bc">
            <mklabel_options>PHASE_ORB_EXT INST_MAG TEAM_NAIF</mklabel_options>
            <description> SPICE CK file containing averaged time-varying alignment of the MAG ASC CHU-$CHU with respect to the spacecraft frame, created by NAIF.
            </description>
            <patterns>
                <CHU value="juno_chub_ali">B</CHU>
                <CHU value="juno_chuc_ali">C</CHU>
                <CHU value="juno_chud_ali">D</CHU>
            </patterns>
        </kernel>

        <kernel pattern="juno_[0-9][0-9][0-9][0-9]_v[0-9][0-9].tm">
            <mklabel_options>N/A</mklabel_options>
            <description>N/A
            </description>
        </kernel>
        <kernel pattern="juno_rec_orbit_v[0-9][0-9].orb">
            <mklabel_options>N/A</mklabel_options>
            <description>N/A
            </description>
        </kernel>

    </kernel_list>

    <!-- =========================== -->
    <!-- Meta-kernel                 -->
    <!-- =========================== -->
    <meta-kernel>
        <mk name="juno_$YEAR_v$VERSION.tm">
            <name>
                <pattern length="2">VERSION</pattern>
                <pattern length="4">YEAR</pattern>
            </name>
        </mk>
    </meta-kernel>

    <!-- =========================== -->
    <!-- Orbit number file           -->
    <!-- =========================== -->
    <orbit_number_file>
        <orbnum>
            <pattern>juno_rec_orbit_v[0-9][0-9].orb</pattern>
        </orbnum>
    </orbit_number_file>

</naif-pds4-bundler_configuration>
