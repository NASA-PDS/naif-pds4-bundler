<?xml version='1.0' encoding='UTF-8'?>
<naif-pds4-bundler_configuration>

    <!-- =========================== -->
    <!-- PDS parameters              -->
    <!-- =========================== -->
    <pds_parameters>
        <pds_version>3</pds_version>
    </pds_parameters>

    <!-- =========================== -->
    <!-- Bundle parameters           -->
    <!-- =========================== -->
    <bundle_parameters>
        <producer_name>Marc Costa Sitja</producer_name>
        <institution>NAIF/JPL</institution>
        <volume_id>odsp_1000</volume_id>
        <spice_name>M01</spice_name>
    </bundle_parameters>

    <!-- =========================== -->
    <!-- Mission Parameters          -->
    <!-- =========================== -->
    <mission_parameters>
        <mission_acronym>m01</mission_acronym>
        <mission_name>M01</mission_name>
        <observer>ODY</observer>
        <target>Mars</target>
        <pds3_mission_template>
    <!--                                                                     -->
    <!--                                                                     -->
    <!-- MAKLABEL (ver. 5.1.0+) Mission Template File for the PDS archiving  -->
    <!-- of the 2001 Mars Odyssey.                                           -->
    <!--                                                                     -->
    <!-- Version 1.3 ~~ by Boris Semenov, NAIF/JPL ~~ September 24, 2015     -->
    <!--                                                                     -->
    <!--    Added SSD.                                                       -->
    <!--                                                                     -->
    <!-- Version 1.2 ~~ by Boris Semenov, NAIF/JPL ~~ October 1, 2009        -->
    <!--                                                                     -->
    <!--    Added ASU.                                                       -->
    <!--                                                                     -->
    <!-- Version 1.1 ~~ by Boris Semenov, NAIF/JPL ~~ March 25, 2005         -->
    <!--                                                                     -->
    <!--    Added tags for extended mission.                                 -->
    <!--                                                                     -->
    <!-- Version 1.0 ~~ by Boris Semenov, NAIF/JPL ~~ May 16, 2002           -->
    <!--                                                                     -->
    <!--    Initial version based on MGS template used for PDS archiving.    -->
    <!--                                                                     -->
            <MISSION_NAME>"2001 MARS ODYSSEY"</MISSION_NAME>
            <SPACECRAFT_NAME>"2001 MARS ODYSSEY"</SPACECRAFT_NAME>
            <TARGET_NAME>"MARS"</TARGET_NAME>
            <DATA_SET_ID>"ODY-M-SPICE-6-V1.0"</DATA_SET_ID>
            <PRODUCT_VERSION_TYPE>"ACTUAL"</PRODUCT_VERSION_TYPE>
            <MISSION_PHASE_NAME>"N/A"</MISSION_PHASE_NAME>
            <SOURCE_PRODUCT_ID>"N/A"</SOURCE_PRODUCT_ID>
            <INSTRUMENT_NAME>"N/A"</INSTRUMENT_NAME>
            <PLATFORM_OR_MOUNTING_NAME>"N/A"</PLATFORM_OR_MOUNTING_NAME>
            <NOTE>"See comments in the file for details"</NOTE>
            <maklabel_options>
                <!--                                                         -->
                <!-- Label key for mission phases.                           -->
                <!--                                                         -->
                <!-- From ODY MISSION.CAT, ver. "2002-03-26 GEO:Slavney"     -->
                <!--                                                         -->
                <!--                                                         -->
                <!--    PRELAUNCH                                            -->
                <!--      Mission Phase Start Time : 2001-01-04              -->
                <!--      Mission Phase Stop Time  : 2001-04-07              -->
                <!--                                                         -->
                <!--    LAUNCH AND INITIALIZATION                            -->
                <!--      Mission Phase Start Time : 2001-04-07              -->
                <!--      Mission Phase Stop Time  : 2001-04-07              -->
                <!--                                                         -->
                <!--    CRUISE                                               -->
                <!--      Mission Phase Start Time : 2001-04-07              -->
                <!--      Mission Phase Stop Time  : 2001-10-23              -->
                <!--                                                         -->
                <!--        Subphases             Dates                      -->
                <!--        ~~~~~~~~~             ~~~~~                      -->
                <!--        Near-Earth            2001-04-07 to 2001-04-21   -->
                <!--        Earth-Mars            2001-04-21 to 2001-09-04   -->
                <!--        Mars Approach         2001-09-04 to 2001-10-17   -->
                <!--                                                         -->
                <!--    ORBIT INSERTION                                      -->
                <!--      Mission Phase Start Time : 2001-10-23              -->
                <!--      Mission Phase Stop Time  : 2001-10-27              -->
                <!--                                                         -->
                <!--    AEROBRAKING                                          -->
                <!--      Mission Phase Start Time : 2001-10-27              -->
                <!--      Mission Phase Stop Time  : 2002-02-19              -->
                <!--                                                         -->
                <!--    MAPPING                                              -->
                <!--      Mission Phase Start Time : 2002-02-19              -->
                <!--      Mission Phase Stop Time  : 2004-08-24              -->
                <!--                                                         -->
                <!--    EXTENDED                                             -->
                <!--      Mission Phase Start Time : 2004-08-25              -->
                <!--      Mission Phase Stop Time  : XXXX-XX-XX              -->
                <!--                                                         -->
                <EXTENDED>
                    <MISSION_PHASE_NAME>"EXTENDED"</MISSION_PHASE_NAME>
                </EXTENDED>
                <!--                                                         -->
                <!-- Label keys for producer IDs                             -->
                <!--                                                         -->
                <NAIF>
                    <PRODUCER_ID>"NAIF/JPL"</PRODUCER_ID>
                </NAIF>
                <ODYNAV>
                    <PRODUCER_ID>"ODYNAV/JPL"</PRODUCER_ID>
                </ODYNAV>
                <IPNG>
                    <PRODUCER_ID>"IPNG/JPL"</PRODUCER_ID>
                </IPNG>
                <!--                                                         -->
                <!-- Label keys for product version types                    -->
                <!--                                                         -->
                <ACTUAL>
                    <PRODUCT_VERSION_TYPE>"ACTUAL"</PRODUCT_VERSION_TYPE>
                </ACTUAL>
                <PREDICT>
                    <PRODUCT_VERSION_TYPE>"PREDICT"</PRODUCT_VERSION_TYPE>
                </PREDICT>
                <!--                                                         -->
                <!-- Label keys for instrument/mounting platform names.      -->
                <!--                                                         -->
                <SC>
                    <INSTRUMENT_NAME>"ODY SPACECRAFT"</INSTRUMENT_NAME>
                    <PLATFORM_OR_MOUNTING_NAME>"N/A"</PLATFORM_OR_MOUNTING_NAME>
                </SC>
                <SA>
                    <INSTRUMENT_NAME>"ODY SOLAR ARRAY"</INSTRUMENT_NAME>
                    <PLATFORM_OR_MOUNTING_NAME>"ODY SPACECRAFT"</PLATFORM_OR_MOUNTING_NAME>
                </SA>
                <HGA>
                    <INSTRUMENT_NAME>"ODY HIGH GAIN ANTENNA"</INSTRUMENT_NAME>
                    <PLATFORM_OR_MOUNTING_NAME>"ODY SPACECRAFT"</PLATFORM_OR_MOUNTING_NAME>
                </HGA>
            </maklabel_options>
        </pds3_mission_template>
        <kernels_to_load>
            <lsk>naif[0-9][0-9][0-9][0-9].tls</lsk>
            <sclk>ORB1_SCLKSCET.[0-9]{5}.tsc</sclk>
            <fk>m01_v[0-9][0-9].tf</fk>
        </kernels_to_load>
    </mission_parameters>

    <!-- =========================== -->
    <!-- Directories                 -->
    <!-- =========================== -->
    <directories>
        <working_directory>working</working_directory>
        <kernels_directory>kernels</kernels_directory>
        <staging_directory>staging</staging_directory>
        <bundle_directory>bundle</bundle_directory>
        <orbnum_directory>kernels/spk</orbnum_directory>
    </directories>

    <!-- =========================== -->
    <!-- Kernel List                 -->
    <!-- =========================== -->
    <kernel_list>
        <kernel pattern="orb1_sclkscet_[0-9][0-9][0-9][0-9][0-9].tsc">
            <mapping>ORB1_SCLKSCET.$VERSION.tsc</mapping>
            <mklabel_options>NAIF SC</mklabel_options>
            <description>This file is a SPICE spacecraft clock (SCLK) kernel containing information required for M01 spacecraft on-board clock to UTC conversion.</description>
            <patterns>
                <VERSION pattern="KERNEL">ORB1_SCLKSCET.$VERSION.tsc</VERSION>
            </patterns>
        </kernel>
        <kernel pattern="m01_hga_ext[0-9][0-9].bc">
            <mklabel_options>NAIF HGA EXTENDED</mklabel_options>
            <description>This file contains orientation and angular rate data for the Mars'01 Odyssey (M01) High Gain Antenna (HGA) Inner and Outer gimbal frames for the extended phase of the mission.
            </description>
        </kernel>
        <kernel pattern="m01_sa_ext[0-9][0-9].bc">
            <mklabel_options>NAIF SA EXTENDED</mklabel_options>
            <description>This file contains orientation and angular rate data for the Mars'01 Odyssey (M01) High Solar Array (SA) Inner and Outer gimbal frames for the extended phase of the mission.
            </description>
        </kernel>
        <kernel pattern="m01_sc_ext[0-9][0-9].bc">
            <mklabel_options>NAIF SC EXTENDED</mklabel_options>
            <description>This file contains orientation and angular velocity data for the Mars-01 Odyssey (M01) spacecraft frame, 'M01_SPACECRAFT', relative to the 'MARSIAU' inertial frame.
            </description>
        </kernel>
        <kernel pattern="m01_sc_ext[0-9][0-9]_rec_nadir.bc">
            <mklabel_options>NAIF SC EXTENDED PREDICT</mklabel_options>
            <description>This file contains predicted nominal nadir-pointed orientation for the Mars'01 Odyssey (M01) spacecraft. Refer to the file's internal comments for details regarding intended usage of this CK.
            </description>
        </kernel>
        <kernel pattern="m01_ext[0-9][0-9].bsp">
            <mklabel_options>ODYNAV EXTENDED</mklabel_options>
            <description>This file contains final merged ephemeris data for the 2001 Mars Odyssey (M01) spacecraft during the extended mission phase.
            </description>
        </kernel>
        <kernel pattern="m01_ext[0-9][0-9]_ipng_mgs95j.bsp">
            <mklabel_options>IPNG EXTENDED</mklabel_options>
            <description>This file contains ephemeris data for the 2001 Mars Odyssey (M01) spacecraft for a portion of the extended mission phase. It was created by the IPNG group, JPL using the Mars Gravity Model MGS95J.
            </description>
        </kernel>
        <kernel pattern="m01_[0-9][0-9][0-9][0-9]_v[0-9][0-9].tm">
            <mklabel_options>N/A</mklabel_options>
            <description>N/A
            </description>
        </kernel>
    </kernel_list>

    <!-- =========================== -->
    <!-- Meta-kernel                 -->
    <!-- =========================== -->
    <meta-kernel>
        <mk_inputs>
            <file>../data/m01_2020_v03.tm</file>
        </mk_inputs>
         <mk name="m01_$YEAR_v$VERSION.tm">
            <name>
                <pattern length="4">YEAR</pattern>
                <pattern length="2">VERSION</pattern>
            </name>
         </mk>
    </meta-kernel>

    <!-- =========================== -->
    <!-- Orbit number file           -->
    <!-- =========================== -->
    <orbit_number_file>
        <orbnum>
            <pattern>m01_ext[0-9]{2}.nrb</pattern>
        </orbnum>
    </orbit_number_file>

</naif-pds4-bundler_configuration>
